{
  "name": "fluid",
  "version": "1.0.0",
  "dependencies": {
    "@microsoft/signalr": "^7",
    "midimessage": "^1",
    "screenfull": "^5",
    "webmidi": "^2"
  },
  "scripts": {
    "start": "parcel src/index.html --port 4321",
    "build": "parcel build src/index.html"
  },
  "devDependencies": {
    "@parcel/packager-xml": "^2.16.3",
    "@parcel/transformer-glsl": "^2.10.3",
    "parcel": "^2.10.3",
    "parcel-config-pwa-manifest": "^0.1.6",
    "process": "^0.11.10"
  },
  "description": "Fluid sim from PavelDoGreat modified to work over websockets and with MIDI support.",
  "pwaManifest": {
    "name": "Fluid!",
    "description": "Fluid sim from PavelDoGreat modified to work over websockets and with MIDI support.",
    "shortName": "Fluid",
    "theme": "black",
    "startURL": "/",
    "display": "fullscreen",
    "generateIconOptions": {
      "baseIcon": "./src/logo.svg",
      "sizes": [
        96,
        152,
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  }
}
