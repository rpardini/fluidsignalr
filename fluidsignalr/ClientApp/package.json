{
  "name": "fluid",
  "version": "1.0.0",
  "main": "src/fluid.js",
  "dependencies": {
    "@microsoft/signalr": "^7",
    "midimessage": "^1",
    "screenfull": "^5",
    "webmidi": "^2"
  },
  "scripts": {
    "start": "parcel src/index.html -p 4321",
    "build": "parcel build src/index.html"
  },
  "devDependencies": {
    "glslify-bundle": "^5",
    "glslify-deps": "^1",
    "parcel-bundler": "^1",
    "parcel-plugin-pwa-manifest": "^3",
    "parcel-plugin-url-loader": "^1"
  },
  "description": "Fluid sim from PavelDoGreat modified to work over websockets and with MIDI support.",
  "pwaManifest": {
    "name": "Fluid!",
    "description": "Fluid sim from PavelDoGreat modified to work over websockets and with MIDI support.",
    "shortName": "Fluid",
    "theme": "black",
    "start_url": "/",
    "display": "fullscreen",
    "generateIconOptions": {
      "baseIcon": "./src/logo.svg",
      "sizes": [
        96,
        152,
        192,
        384,
        512
      ],
      "genFavicons": true
    }
  }
}
